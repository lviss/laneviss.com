{
  "hosting": {
    "public": "dist/laneviss-com",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/ip",
        "function": "ip"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
